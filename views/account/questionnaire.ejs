                                            <input type="text" class="form-control border border-info" name="lastName1" id="lastName1" value="<%=data[0].lastName1%>" placeholder="สกุล"> </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-5">
                                        <div class="mb-3">
